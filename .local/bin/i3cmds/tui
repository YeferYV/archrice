tui(){
  #echo -e "\n"
  echo ' '
  /usr/local/bin/Xsixel :1 -exec urxvt 2>/dev/null
  #/usr/local/bin/Xsixel :1 -exec urxvt
}
#tui
tui2(){
  echo ''
  /usr/local/bin/Xsixel :1 -exec urxvt 2>/dev/null
  #lsix2 Pictures/redmoon.jpg
  echo ''
}
#tui2
tui3(){
  #/usr/local/bin/Xsixel :1 -exec urxvt 2>/dev/null
  #echo "       $(lsix2 ~/Pictures/redmoon.jpg)"
  echo "       $(lsix2 $1  $2)"
  #echo "                    $(lsix2 $1.png  $2.png)"
  echo ' '
}
tui3 $1 $2
